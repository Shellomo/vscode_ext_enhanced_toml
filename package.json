{
  "name": "enhanced-toml",
  "displayName": "Enhanced TOML",
  "description": "TOML language support for VS Code",
  "version": "1.0.4",
  "publisher": "Shellomo",
  "icon": "media/logo.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "toml",
        "aliases": [
          "TOML",
          "toml"
        ],
        "extensions": [
          ".toml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "toml",
        "scopeName": "source.toml",
        "path": "./syntaxes/toml.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "vscode-toml.formatDocument",
        "title": "Format TOML Document"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:toml",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^14.x.x",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.x.x"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@vscode/extension-telemetry": "^0.9.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shellomo/vscode_ext_enhanced_toml"
  },
  "keywords": [
    "toml",
    "language",
    "syntax",
    "highlighting",
    "formatting"
  ]
}